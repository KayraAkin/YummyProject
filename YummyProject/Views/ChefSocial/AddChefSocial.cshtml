@model YummyProject.Models.ChefSocial
@{
    ViewBag.Title = "AddChefSocial";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<h2>Yeni Sosyal Medya Ekleme Sayfası</h2>

<form method="post">

    <input type="hidden" name="ChefId" value="@ViewBag.chefId" />
    <label>Url</label>
    @Html.TextAreaFor(x => x.Url, new { @class = "form-control" })
    <div class="text-danger">@Html.ValidationMessageFor(x => x.Url)</div>
    <br />
    <label>Sosyal Medya</label>
    @Html.TextAreaFor(x => x.SocialMadiaName, new { @class = "form-control" })
    <div class="text-danger">@Html.ValidationMessageFor(x => x.SocialMadiaName)</div>
    <br />
    <label>İkon</label>
    @Html.TextAreaFor(x => x.Icon, new { @class = "form-control" })
    <div class="text-danger">@Html.ValidationMessageFor(x => x.Icon)</div>
    <br />


    <div class="text-danger">@ViewBag.error</div>
    <button class="btn btn-primary">Kaydet</button>

</form>

